{
    "env": {
      "node": true,
      "browser": true,
      "es2020": true
    },
    "extends": [
      "airbnb-base",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-param-reassign": "off",
      "import/extensions": "off",
      "no-underscore-dangle": "off"
    }
}
